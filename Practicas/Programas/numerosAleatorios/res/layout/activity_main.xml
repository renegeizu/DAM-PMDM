<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/LinearLayout1"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".MainActivity" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <EditText
            android:id="@+id/et_Primero"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:ems="10"
            android:hint="@string/hint_et_Primero"
            android:text="" >

            <requestFocus />
        </EditText>

        <EditText
            android:id="@+id/et_Ultimo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:ems="10"
            android:hint="@string/hint_et_Ultimo"
            android:text="" />

        <EditText
            android:id="@+id/et_Cantidad"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:ems="10"
            android:hint="@string/hint_et_Cantidad" />

    </LinearLayout>

    <Button
        android:id="@+id/bt_Generar"
        android:layout_width="288dp"
        android:layout_height="wrap_content"
        android:onClick="generar"
        android:text="@string/bt_Generar" />

    <TextView
        android:id="@+id/tv_Numeros"
        android:layout_width="290dp"
        android:layout_height="wrap_content"
        android:layout_weight="0.36"
        android:text="@string/tv_Numero"
        android:textAppearance="?android:attr/textAppearanceLarge" />

</LinearLayout>